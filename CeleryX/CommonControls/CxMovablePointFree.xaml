<Thumb x:Class="CeleryX.CommonControls.CxMovablePointFree"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:CeleryX.CommonControls"
             mc:Ignorable="d"
       Name="ThMovPointFree" Background="White" Foreground="Black" Height="8" Width="8">
    <Thumb.Template>
        <ControlTemplate TargetType="Thumb">
            <Rectangle Fill="#FFFFF7E6" Stroke="Black" StrokeThickness="0.5"/>
        </ControlTemplate>
    </Thumb.Template>
</Thumb>
